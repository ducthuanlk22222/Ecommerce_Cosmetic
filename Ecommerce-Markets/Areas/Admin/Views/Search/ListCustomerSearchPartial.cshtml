@model List<Customer>

@if (Model != null)
{
    foreach (var item in Model)
    {
        <tr>
            <td>@item.CustomerId </td>
            <td>
                <p>@item.FullName</p>
            </td>
            <td>
                <p>@item.Phone</p>
            </td>
            <td>
                <p>@item.Email</p>
            </td>
            <td>@item.Address</td>
            @if (@item.Location != null)
            {
                <td>@item.Location.Name</td>
            }

            <td>
                <a class="btn btn-warning m-r-5" asp-area="Admin" asp-controller="AdminCustomers" asp-action="Details" asp-route-id="@item.CustomerId">View</a>
                <a class="btn btn-success m-r-5" asp-area="Admin" asp-controller="AdminCustomers" asp-action="Edit" asp-route-id="@item.CustomerId">Edit</a>
                <a OnClientClick='javascript:window.open(' Default2.aspx','List','scrollbars=no,resizable =no,width =400,height =280'); return false;' class="btn btn-danger m-r-5" asp-area="Admin" asp-controller="AdminCustomers" asp-action="Delete" asp-route-id="@item.CustomerId">Delete</a>

            </td>
        </tr>
    }
}

